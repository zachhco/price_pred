{
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.py$1",
        "methods": ["GET", "POST"]
      }
    ],
    "env": {
      "FLASK_APP": "index.py",
      "FLASK_ENV": "production"
    },
    "build": {
      "env": {
        "PIP_TARGET": "/vercel/path0",
        "PYTHONUNBUFFERED": "1"
      }
    }
  }